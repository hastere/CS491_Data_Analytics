   "outputs": [],
   "source": [
    "#TODO: provide the image name for classification\n",
    "#img_path = 'Image_name_here.jpg'\n",
    "#TODO Ends\n",
    "img_path = '1.jpg'\n",
    "img = image.load_img(img_path, target_size=(224, 224))\n",
    "x = image.img_to_array(img)\n",
    "x = np.expand_dims(x, axis=0)\n",
    "x = preprocess_input(x)\n"
   ]
  },
  {
   "cell_type": "code",
